RewriteEngine On
RewriteCond %{HTTPS} off [OR]
RewriteCond %{HTTP_HOST} ^www. [NC]
RewriteCond %{HTTP_HOST} ^(?:www.)?(.+)$ [NC]
RewriteRule ^ https://%1%{REQUEST_URI} [L,NE]

RewriteCond %{THE_REQUEST} /callback\.php[?/\s] [NC]
RewriteRule ^ - [R=404,L]

RewriteBase /
RewriteCond %{REQUEST_URI} !index\.php [NC]
RewriteRule ^([^/]+)(.*)?callback.png$ callback.php?email=$1&timestamp=$2   [L]